             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
              xmlns:local="clr-namespace:App1.Views"
             x:Class="App1.Views.MainConsolePage"
             MasterBehavior="Split"
                  >
  <MasterDetailPage.Master>
    <local:MainHeaderPage/>
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <local:CoursePage/>
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>